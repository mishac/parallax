{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "> 0.5%",
          "not ie <= 10"
        ]
      }
    }]
  ],
  "plugins": ["syntax-dynamic-import"],
  "retainLines": true,
  "ignore": [
    "src/js/vendor",
    "/node_modules/"
  ]
}
